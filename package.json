{
  "name": "umlsjs",
  "version": "0.1.0",
  "description": "Nodejs package to access UMLS REST web-services",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx babel src --out-dir dist",
    "start": "npx babel src --watch --out-dir dist",
    "release": "np",
    "test": "jest --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dermatologist/umlsjs.git"
  },
  "keywords": [
    "UMLS",
    "REST"
  ],
  "dependencies": {
    "axios": "^0.19.0",
    "fast-xml-parser": "^3.15.1",
    "he": "^1.2.0",
    "querystring": "^0.2.0",
    "regenerator-runtime": "^0.13.3",
    "socks-proxy-agent": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.7.7",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0"
  },
  "author": "Bell Eapen",
  "license": "MIT",
  "np": {
    "yarn": false,
    "contents": "dist",
    "releaseDraft": false,
    "anyBranch": true
  },
  "bugs": {
    "url": "https://github.com/dermatologist/umlsjs/issues"
  },
  "homepage": "https://github.com/dermatologist/umlsjs#readme"
}
